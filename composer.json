{
    "name": "app/foundation",
    "license": "proprietary",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9.1"
    },
    "autoload": {
        "files": [
            "src/Foundation/helpers.php"
        ],
        "psr-4": {
            "App\\Foundation\\": "src/Foundation"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
